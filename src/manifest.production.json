{
    "manifest_version": 2,
    "name": "BookShelf",
    "description": "chrome extension",
    "version": "0.0.1",
    "options_page": "options.html",
    "browser_action": {
        "default_icon": "icons/favorite.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "bookmarks",
        "chrome://favicon/",
        "fontSettings",
        "tabs",
        "http://*/*",
        "https://*/*"
    ]
}